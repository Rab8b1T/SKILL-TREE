{
  "builds": [
    {
      "src": "contest_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "contest_server.py"
    },
    {
      "src": "/contest/(.*)",
      "dest": "/contest/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "DB_NAME": "skilltree"
  }
}
